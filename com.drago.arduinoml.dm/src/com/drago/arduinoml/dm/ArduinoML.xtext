grammar com.drago.arduinoml.dm.ArduinoML with org.eclipse.xtext.common.Terminals

generate arduinoML "http://www.drago.com/arduinoml/dm/ArduinoML"

Domainmodel :
    (elements+=Type)*;
  
Type:
    DataType | Entity;
  
DataType:
    'datatype' name=ID;
 
Entity:
    'entity' name=ID ('extends' superType=[Entity])? '{'
        (features+=Feature)*
    '}';
 
Feature:
    (many?='many')? name=ID ':' type=[Type];