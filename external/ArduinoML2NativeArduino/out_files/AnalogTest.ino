// Structural concepts
void setup() {
	pinMode(0, INPUT);
	pinMode(12, OUTPUT);
}

//Behavioral concepts
long time=0; long debounce = 200;

void state_off() {
	digitalWrite(12, LOW);
	boolean guard = millis() - time > debounce;
	if( analogRead(0) >= 500 && guard ) {
		delay(0);
		time = millis();
		state_on();
	}
	else {
		state_off(); 
	}
}

void state_on() {
	digitalWrite(12, HIGH);
	boolean guard = millis() - time > debounce;
	if( analogRead(0) < 500 && guard ) {
		delay(0);
		time = millis();
		state_off();
	}
	else {
		state_on(); 
	}
}

void loop() {state_off();} // Entering init state